{
  "extends": ["tslint-consistent-codestyle", "tslint-config-airbnb"],
  "rules": {
    "import-name": false,
    "naming-convention": [
      true,
      {"type": "variable", "format": "camelCase"},
      {"type": "variable", "modifiers": "const", "format": ["camelCase", "UPPER_CASE"], "final": true},
      {"type": "variable", "modifiers": "export", "format": "PascalCase", "final": true}
    ],
    "object-shorthand-properties-first": false,
    "space-before-function-paren": [true, "never"],
    "ter-prefer-arrow-callback": false,
    "variable-name": false
  }
}
